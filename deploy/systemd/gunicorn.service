[Unit]
Description=Gunicorn for Django (CB)
After=network.target

[Service]
User=tester
Group=www-data
WorkingDirectory=/home/tester/djangowork/teamproject/CB/CB
Environment="PATH=/home/tester/djangowork/teamproject/CB/venv/bin"
ExecStart=/home/tester/djangowork/teamproject/CB/venv/bin/gunicorn CB.wsgi:application --bind 127.0.0.1:8000 --workers 3

Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
